{
    "name": "frontend/jquery-framework",
    "description": "A Laravel-like frontend framework for jQuery",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Eslam Mohamed"
        }
    ],
    "autoload": {
        "psr-4": {
            "JqueryFramework\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JqueryFramework\\Providers\\JqueryFrameworkServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0"
    }
}