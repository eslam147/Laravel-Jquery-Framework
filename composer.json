{
    "name": "frontend/jquery-framework",
    "description": "A Laravel-like frontend framework for jQuery",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Eslam Mohamed"
        }
    ],
    "autoload": {
        "psr-4": {
            "JqueryFramework\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JqueryFramework\\Providers\\JqueryFrameworkServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Jquery-Framework",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    }
}